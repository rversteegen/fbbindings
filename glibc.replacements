convert c: #define __PTHREAD_SPINS 0, 0
to fb:     #define __PTHREAD_SPINS 0, 0

# Looks like the {...} initializer here refers to the
# pthread_rwlock_t___data.__pad1 field (64bit only), which is an array.
convert c: #define __PTHREAD_RWLOCK_ELISION_EXTRA 0, { 0, 0, 0, 0, 0, 0, 0 }
to fb:     #define __PTHREAD_RWLOCK_ELISION_EXTRA 0, { 0, 0, 0, 0, 0, 0, 0 }

convert c: #define __REDIRECT(name, proto, alias) name proto __asm__ (__ASMNAME (#alias))
to fb:      #define __REDIRECT(name, proto, alias) name proto asm__ (__ASMNAME (#alias))

convert c: typedef int         int8_t __attribute__ ((__mode__ (__QI__)));
to c:      typedef signed char int8_t;

convert c: typedef int   int16_t __attribute__ ((__mode__ (__HI__)));
to c:      typedef short int16_t;

convert c: typedef int int32_t __attribute__ ((__mode__ (__SI__)));
to c:      typedef int int32_t;

convert c: typedef int       int64_t __attribute__ ((__mode__ (__DI__)));
to c:      typedef long long int64_t;

convert c: typedef unsigned int  u_int8_t __attribute__ ((__mode__ (__QI__)));
to c:      typedef unsigned char u_int8_t;

convert c: typedef unsigned int   u_int16_t __attribute__ ((__mode__ (__HI__)));
to c:      typedef unsigned short u_int16_t;

convert c: typedef unsigned int u_int32_t __attribute__ ((__mode__ (__SI__)));
to c:      typedef unsigned int u_int32_t;

convert c: typedef unsigned int       u_int64_t __attribute__ ((__mode__ (__DI__)));
to c:      typedef unsigned long long u_int64_t;

